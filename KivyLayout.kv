<ScreenManager>
    SolverScreen
    SettingScreen

<SolverScreen>:
    name: "solver"
    Button:
        text: "Run The Solver"
        size_hint: .5, .5
        on_release: root.runSolver()

<SettingScreen>:
    name: "setting"
    Button:
        text: "Back"
        on_release: root.manager.current = "solver"